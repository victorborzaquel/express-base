{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "allowUnreachableCode": true,
    "noImplicitAny": false,
    // "strict": true,
    // "strictNullChecks": false
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
